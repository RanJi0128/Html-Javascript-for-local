      "agency_field_main_message": [
        "<div class=\"header--main--message\">We\u2019re an award-winning digital agency with a team of designers &amp; creative developers who relish in crafting engaging experiences<\/div>"
      ],
      "agency_field_text_title": [
        "<h2 class=\"hoverJump\"><span class=\"split-it-all\">Your digital<\/span>\r\n<span class=\"split-it-all gray--part\">partner<\/span><\/h2>"
      ],
      "agency_field_text_content": [
        "<p>Our multidisciplinary team is led by our creative director who is part of the AWWWARDS Jury and encourages every team member to discover, define and create innovative ways to help connect brands with people.<\/p>\r\n\r\n<p>\r\nWe're collaborators by nature and we work closely with our international clients to deliver cutting-edge digital experiences, we believe in working \u2018with\u2019 you rather than \u2018for\u2019 you. Nothing holds us back to venture and experiment where technology hasn\u2019t been yet and making exciting, immersive work, is what drives our team daily.\r\n<\/p>\r\n\r\n<p>\r\nWe have a wealth of experience providing digital expertise and we do this by revealing insights that will help support and grow your business. We love exploring, brainstorming and prototyping allowing us to choose the best use of innovative technologies that are right for your digital project.\r\n<\/p>"
      ],
      "agency_field_text_lists": [
        "<div class=\"col-12 col-md-4 col\">\r\n<h4>Conception<\/h4>\r\n<ul>\r\n \t<li>Digital Strategy<\/li>\r\n \t<li>Ideation &amp; Prototyping<\/li>\r\n \t<li>Brand &amp; Product Strategy<\/li>\r\n \t<li>Technical Direction<\/li>\r\n \t<li>Project Management<\/li>\r\n<\/ul>\r\n<\/div>\r\n<div class=\"col-12 col-md-4 col\">\r\n<h4>Design<\/h4>\r\n<ul>\r\n \t<li>Creative Art Direction<\/li>\r\n \t<li>Interactive Design<\/li>\r\n \t<li>UI\/UX Web Design<\/li>\r\n \t<li>Motion Design<\/li>\r\n \t<li>2D\/3D Design<\/li>\r\n<\/ul>\r\n<\/div>\r\n<div class=\"col-12 col-md-4 col\">\r\n<h4>Development<\/h4>\r\n<ul>\r\n \t<li>Creative Development<\/li>\r\n \t<li>Front-end Development<\/li>\r\n \t<li>Back-end Development<\/li>\r\n \t<li>Web App Development<\/li>\r\n \t<li>Mobile Development<\/li>\r\n<\/ul>\r\n<\/div>"
      ],
